{
  "name": "City",
  "plural": "cities",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "Number",
      "id": true,
      "generated": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "required": true
    },
    "point": {
      "type": "geopoint"
    },
    "bound": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "streets": {
      "type": "hasMany",
      "model": "Street",
      "foreignKey": "cityId"
    },
    "districts": {
      "type": "hasMany",
      "model": "District",
      "foreignKey": "cityId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
