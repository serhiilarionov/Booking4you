{
  "name": "Client",
  "plural": "clients",
  "base": "User",
  "idInjection": true,
  "options": {
    "base": "User",
    "strict": true,
    "validateUpsert": true
  },
  "hidden": [],
  "properties": {
    "phone": {
      "type": "string",
      "required": false
    },
    "phoneVerified": {
      "type": "date",
      "required": false,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "companies": {
      "type": "hasMany",
      "model": "Company",
      "foreignKey": "clientId",
      "through": "Favorite"
    },
    "bookings": {
      "type": "hasMany",
      "model": "Booking",
      "foreignKey": "clientId"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
