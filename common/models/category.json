{
  "name": "Category",
  "plural": "categories",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "deletedAt"
  ],
  "properties": {
    "id": {
      "type": "Number",
      "id": true,
      "generated": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "parentId": {
      "type": "number"
    },
    "icon": {
      "type": "string"
    },
    "position": {
      "type": "number",
      "required": true,
      "default": "1"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "deletedAt": {
      "type": "date"
    }
  },
  "scope": {
    "limit": 1000
  },
  "validations": [],
  "relations": {
    "companies": {
      "type": "hasMany",
      "model": "Company",
      "foreignKey": "categoryId",
      "through": "CategoryCompany"
    },
    "services": {
      "type": "hasMany",
      "model": "CompanyService",
      "foreignKey": "categoryId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
