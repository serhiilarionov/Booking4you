{
  "name": "Company",
  "plural": "companies",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "createdAt",
    "updatedAt",
    "deletedAt"
  ],
  "properties": {
    "id": {
      "type": "Number",
      "id": true,
      "generated": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "categoryId": {
      "type": "number",
      "required": true
    },
    "desc": {
      "type": "string"
    },
    "photo": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "updatedAt": {
      "type": "date",
      "default": "$now"
    },
    "deletedAt": {
      "type": "date",
      "required": false,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "details": {
      "type": "hasOne",
      "model": "CompanyDetail",
      "foreignKey": "companyId"
    },
    "location": {
      "type": "hasOne",
      "model": "CompanyLocation",
      "foreignKey": "companyId"
    },
    "service": {
      "type": "hasOne",
      "model": "CompanyService",
      "foreignKey": "companyId"
    },
    "bookings": {
      "type": "hasMany",
      "model": "Booking",
      "foreignKey": "companyId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
