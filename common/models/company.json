{
  "name": "Company",
  "plural": "companies",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "createdAt",
    "updatedAt",
    "deletedAt"
  ],
  "properties": {
    "id": {
      "type": "Number",
      "id": true,
      "generated": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "desc": {
      "type": "string"
    },
    "photo": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "point": {
      "type": "geopoint"
    },
    "buildingId": {
      "type": "number"
    },
    "streetId": {
      "type": "number"
    },
    "districtId": {
      "type": "number"
    },
    "cityId": {
      "type": "number"
    },
    "room": {
      "type": "string"
    },
    "locationDetail": {
      "type": "string"
    },
    "fullAddress": {
      "type": "string"
    },
    "verifiedAt": {
      "type": "date",
      "default": "$now"
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "updatedAt": {
      "type": "date",
      "default": "$now"
    },
    "deletedAt": {
      "type": "date",
      "required": false,
      "default": "$now"
    }
  },
  "scope": {
    "limit": 100
  },
  "validations": [],
  "relations": {
    "details": {
      "type": "hasOne",
      "model": "CompanyDetail",
      "foreignKey": "companyId"
    },
    "location": {
      "type": "hasOne",
      "model": "CompanyLocation",
      "foreignKey": "companyId"
    },
    "service": {
      "type": "hasMany",
      "model": "CompanyService",
      "foreignKey": "companyId"
    },
    "bookings": {
      "type": "hasMany",
      "model": "Booking",
      "foreignKey": "companyId"
    },
    "categories": {
      "type": "hasMany",
      "model": "Category",
      "foreignKey": "companyId",
      "through": "CategoryCompany"
    },
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "categoryId"
    },
    "clients": {
      "type": "hasMany",
      "model": "Client",
      "foreignKey": "companyId",
      "through": "Favorite"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
