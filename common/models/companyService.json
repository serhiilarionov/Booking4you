{
  "name": "CompanyService",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [],
  "properties": {
    "id": {
      "type": "Number",
      "id": true,
      "generated": true
    },
    "name": {
      "type": "String",
      "required": true
    },
    "desc": {
      "type": "String"
    },
    "price": {
      "type": "Number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2
    },
    "salePrice": {
      "type": "Number",
      "dataType": "decimal",
      "precision": 10,
      "scale": 2
    },
    "currency": {
      "type": "String"
    },
    "position": {
      "type": "Number",
      "required": true
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "deletedAt": {
      "type": "date",
      "required": false,
      "default": "$now"
    }
  },
  "scope": {
    "limit": 1000
  },
  "validations": [],
  "relations": {
    "company": {
      "type": "belongsTo",
      "model": "Company",
      "foreignKey": "companyId"
    },
    "bookings": {
      "type": "hasMany",
      "model": "Booking",
      "foreignKey": "serviceId",
      "through": "BookingService"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
