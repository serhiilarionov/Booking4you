{
  "name": "Street",
  "plural": "streets",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "point": {
      "type": "geopoint"
    },
    "bound": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "streetType": {
      "type": "belongsTo",
      "model": "StreetType",
      "foreignKey": "streetTypeId"
    },
    "buildings": {
      "type": "hasMany",
      "model": "Building",
      "foreignKey": "streetId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
